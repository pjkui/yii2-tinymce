{
    "name": "pjkui/yii2-tinymce",
    "description": "tinymce integration for yii2",
    "keywords": [
        "yii2",
        "widget",
        "yii2 widget",
        "tinymce"
    ],
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "quinn pan",
            "email": "pjkui@qq.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.14",
        "tinymce/tinymce": "~5.10.2"
    },
    "suggest": {
        "mihaildev/yii2-elfinder": "tinymce can use elfinder as file manager, authors recommend this library, ^1.4"
    },
    "autoload": {
        "psr-4": {
            "pjkui\\Yii2Tinymce\\": "src/"
        }
    }
}
